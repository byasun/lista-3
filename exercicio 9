import java.util.*;
public class Main{
 public static void main (String[] args) { 
  Scanner scan = new Scanner (System.in);
   int n; 
   int ma = n;
   int me = n;
   for (int i=0; i < 10; i++) {
    System.out.println("Digite um número inteiro: ");
    n = scan.nextInt();
    if (n > ma) {
      ma = n;
    }
    if (n < me) {
      me = n;
    }
   }
    System.out.println("o maior valor informado é " + ma);
    System.out.println("o menor valor informado é "  + me);
 }
}
